#include <stdio.h>
#include <ctype.h>
#include <locale.h>
#include <time.h>

int main() {
    setlocale(LC_ALL,"Rus");
    FILE *fp; // указатель на файл
    char ch; // текущий символ
    int num = 0; // переменная для проверки числа
    int flag = 1; // флаг для определения наличия двузначных чисел
    fp = fopen("test.txt", "r"); // открытие файла на чтение
    if (fp == NULL) { // если файл не удалось открыть
        printf("Не удалось открыть файл.");
        //exit(1);
    }
    while ((ch = fgetc(fp)) != EOF) { // читаем символы до конца файла
        if (ch >= '0' && ch <= '9') { // если символ является цифрой
            num = num * 10 + (ch - '0'); // формируем числа
            if (num >= 10 && num <= 99) { // если число двузначное
                flag = 0; // флаг наличия двузначных чисел
            }
        }
        else if (ch == '\n') { // если символ конца строки
            if (flag) { // если в строке нет двузначных чисел
                printf("\n"); // переводим строку
            }
            num = 0; // обнуляем переменную для проверки числа
            flag = 1; // обнуляем флаг
        }
    }
    fclose(fp); // закрываем файл
    return 0;
}
