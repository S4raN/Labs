#include <stdio.h>
#include <stdlib.h>
#include <time.h>
 
int main(){
    int array[] = {15, 2, 333, 4, 52, 6, 777, 8, 99};
    size_t size = sizeof(array) / sizeof(*array);
    size_t i;
 
    FILE* f = fopen("Numbers", "w");
 
    if (f == NULL){
        perror("file.txt");
        return 1;}
 
    for (i = 0; i < size; ++i){
        fprintf(f, "%d\n", array[i]);}
 
 
 
 
 
 
    fclose(f);
    return 0;}
