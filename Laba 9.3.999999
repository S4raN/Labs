#include <string.h>
#include <ctype.h>
#include<locale.h>

#define MAX_LEN 1024

int has_two_digits(const char *str){
    /* Функция, которая проверяет строку на наличие двузначных чисел */
    int len = strlen(str);
    int i = 0;
    for (i = 0; i < len - 1; i++){
        if (isdigit(str[i]) && isdigit(str[i + 1])){
            int num = (str[i] - '0') * 10 + (str[i + 1] - '0');
            if (num >= 10 && num <= 99){
                return 1;}}}
    return 0;}

int main(){
    setlocale(LC_ALL,"Rus");
    /* Открываем файл для чтения */
    FILE *fp = fopen("input.txt", "r");

    if (fp == NULL){
        fprintf(stderr, "Не удалось открыть файл input.txt\n");
        return 1;}

    char buffer[MAX_LEN];

    while (fgets(buffer, MAX_LEN, fp)){
        /* Читаем каждую строку в файле */
        if (!has_two_digits(buffer)){
            /* Если в строке нет двузначных чисел, то выводим ее на экран */
            printf("%s", buffer);}}
    fclose(fp); /* Закрываем файл */
    return 0;}
