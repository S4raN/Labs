#include <stdio.h>

int contains_two_digits(int num) {
    return num >= 10 && num <= 99; // возвращает 1, если число содержит две цифры, иначе 0
}

int main() {
    FILE *file;
    file = fopen("numbers.txt", "r"); // открытие файла на чтение

    if (file == NULL) { // если файл не удалось открыть
        printf("Error opening file!\n");
        return 1;
    }

    char line[256];
    while (fgets(line, sizeof(line), file)) { // считываем строки из файла
        int i = 0, num = 0, has_two_digits = 0; // i - индекс текущего символа в строке, num - текущее число в строке, has_two_digits - флаг, показывающий, есть ли двузначные числа в строке
        while (line[i] != '\n') { // пока не достигнут конец строки
            if (line[i] >= '0' && line[i] <= '9') {
                num = num * 10 + (line[i] - '0'); // если текущий символ - цифра, добавляем его к числу
            } else {
                if (contains_two_digits(num)) {
                    has_two_digits = 1; // если текущее число содержит две цифры, устанавливаем флаг
                    break;
                }
                num = 0; // сбрасываем текущее число
            }
            i++;
        }
        if (!has_two_digits && line[0] != '\n') { // если в строке нет двузначных чисел и строка не пустая, выводим ее на экран
            printf("%s", line);
        }
    }

    fclose(file); // закрытие файла
    return 0;
}
